@model Autopark_Web_Version.Models.Details

@{
    ViewData["Title"] = "New Detail";
}
<h2>@ViewData["Title"]</h2>

<form asp-action="Create" asp-controller="Details">
    <div>        
        <div class="form-group">
            <label asp-for="DetailName"></label>
            <input asp-for="DetailName" class="form-control  form-control-sm" />
        </div>       
        
        <div class="form-group">
            <input type="submit" value="Сохранить" class="btn btn-outline-secondary" />
        </div>
    </div>
</form>

<div>
    <a asp-controller="Details" asp-action="Index">Back</a>
</div>
