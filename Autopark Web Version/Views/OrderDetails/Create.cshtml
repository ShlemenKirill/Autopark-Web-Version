@model Autopark_Web_Version.Models.Models.OrderDetails

@{
    ViewData["Title"] = "New Order";
}
<h2>@ViewData["Title"]</h2>

<form asp-action="Create" asp-controller="OrderDetails">
    <div>
        <div class="form-group">
            <label asp-for="OrderId"></label>
            <input asp-for="OrderId" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <label asp-for="VenicleId" ></label>            
            <input asp-for="VenicleId" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">            
            <label asp-for="DetailId"></label>
            <input asp-for="DetailId" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <label asp-for="Quantity"></label>            
            <input asp-for="Quantity" class="form-control  form-control-sm" />
        </div>

        <div class="form-group">
            <input type="submit" value="Сохранить" class="btn btn-outline-secondary" />
        </div>
    </div>
</form>

<div>
    <a asp-controller="Details" asp-action="Index">Back</a>
</div>
