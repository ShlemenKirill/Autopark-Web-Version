@model IEnumerable<Autopark_Web_Version.Models.Models.Orders>

@{
    ViewData["Title"] = "AutoPark";
    ViewData["Type"] = null;
}
<h2>@ViewData["Title"]</h2>
<p>
    <a asp-controller="Details" asp-action="Create">Add</a>
</p>
<table class="table">
    <tr>
        <th><a>Order ID</a></th>
        <th><a>Venicle ID</a></th>
        <th><a>Order Date</a></th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>@item.OrderId</td>
    <td>@item.VenicleId</td>
    <td>@item.Date</td>

    <td>
        <a asp-controller="Orders" asp-action="Edit" asp-route-id="@item.OrderId">Change</a> |
        <a asp-controller="Orders" asp-action="Delete" asp-route-id="@item.OrderId">Delete</a>
        <a asp-controller="Orders" asp-action="Details" asp-route-id="@item.OrderId">Details</a>
    </td>
</tr>
    }
</table>
