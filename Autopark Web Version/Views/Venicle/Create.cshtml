@model Autopark_Web_Version.Models.Venicles

@{
    ViewData["Title"] = "New Venicle";
}
<h2>@ViewData["Title"]</h2>

<form asp-action="Create" asp-controller="Venicle">
    <div>
        <div class="form-group">
            <label asp-for="VeniclesTypeId"></label>
            <select class="form-control" asp-for="VeniclesTypeId">
                <option value="1">Bus</option>
                <option value="2">Car</option>
                <option value="3">Rink</option>
                <option value="4">Tractor</option>
            </select>
        </div>
        <div class="form-group">
            <label asp-for="Engine"></label>
            <select class="form-control" asp-for="Engine">
                <option value="Electric">Electric</option>
                <option value="Gasoline">Gasoline</option>
                <option value="Diesel">Diesel</option>                
            </select>
        </div>        
        <div class="form-group">
            <label asp-for="ModelName"></label>
            <input asp-for="ModelName" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <label asp-for="RegistrationNumber"></label>
            <input asp-for="RegistrationNumber" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <label asp-for="Weight"></label>
            <input asp-for="Weight" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <label asp-for="Year"></label>
            <input asp-for="Year" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <label asp-for="Color"></label>
            <input asp-for="Color" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <label asp-for="Mileage"></label>
            <input asp-for="Mileage" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <label asp-for="Tank"></label>
            <input asp-for="Tank" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <label asp-for="Consumption"></label>
            <input asp-for="Consumption" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <input type="submit" value="Сохранить" class="btn btn-outline-secondary" />
        </div>
    </div>
</form>

<div>
    <a asp-controller="Venicle" asp-action="Index">Back</a>
</div>
